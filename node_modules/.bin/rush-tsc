#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@microsoft/rush-stack-compiler-3.3/bin/rush-tsc" "$@"
else 
  exec node  "$basedir/../@microsoft/rush-stack-compiler-3.3/bin/rush-tsc" "$@"
fi
